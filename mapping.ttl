@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix chr: <https://chromata-project.gr/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .


<chr:AvatarMapping>  a rr:TriplesMap;
  rml:logicalSource [
    rml:source "/home/estathop/Desktop/chromata/datadumpforsync.json";
    rml:referenceFormulation ql:JSONPath;
    rml:iterator "$[*].metadata.[3].metadata.[*]"
  ];
  rr:subjectMap [
    rr:template "https://chromata-project.gr/avatar-{id}";
    rr:class chr:Avatar
  ];
  rr:predicateObjectMap [
    rr:predicate chr:id;
    rr:objectMap [ rml:reference "id" ]
  ];
    rr:predicateObjectMap [
    rr:predicate chr:project_id;
    rr:objectMap [ rml:reference "project_id" ]
  ];
    rr:predicateObjectMap [
    rr:predicate chr:size;
    rr:objectMap [ rml:reference "size" ]
  ];
    rr:predicateObjectMap [
    rr:predicate chr:user_id;
    rr:objectMap [ rml:reference "user_id" ]
  ];
  rr:predicateObjectMap [
    rr:predicate chr:description;
    rr:objectMap [ rml:reference "description" ]
  ].

